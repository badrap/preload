{
  "name": "@badrap/preload",
  "version": "1.0.1",
  "description": "Data preloading for vue-router, similar to Sapper's preload or Nuxt.js's asyncData",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "source": "src/index.js",
  "repository": "badrap/preload",
  "author": "Joachim Viide <jviide@iki.fi>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "lint": "eslint --ignore-path .gitignore --max-warnings 0 src",
    "build": "rm -rf dist && microbundle --format es,cjs",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@vue/test-utils": "^1.1.2",
    "babel-jest": "^26.6.3",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "microbundle": "^0.13.0",
    "prettier": "^2.2.1",
    "vue": "^2.6.12",
    "vue-router": "^3.4.9",
    "vue-template-compiler": "^2.6.12"
  },
  "keywords": [
    "vue",
    "router",
    "routing",
    "preload"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  }
}
