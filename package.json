{
  "name": "@badrap/preload",
  "version": "1.0.1",
  "description": "Data preloading for vue-router, similar to Sapper's preload or Nuxt.js's asyncData",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "source": "src/index.js",
  "repository": "badrap/preload",
  "author": "Joachim Viide <jviide@iki.fi>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "lint": "eslint --ignore-path .gitignore --max-warnings 0 .",
    "build": "rm -rf dist && microbundle --format es,cjs",
    "prepublishOnly": "yarn run build"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.8.0",
    "microbundle": "^0.11.0",
    "prettier": "^1.17.0",
    "vue": "^2.6.10",
    "vue-router": "^3.0.6",
    "vue-template-compiler": "^2.6.10"
  },
  "keywords": [
    "vue",
    "router",
    "routing",
    "preload"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  }
}
